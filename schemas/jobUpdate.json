{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "required": [
        "title"
    ],
    "properties": {
        "title": {
            "type": "string",
            "minLength": 1,
            "maxLength": 30
        },
        "salary": {
            "type": "integer"

        },
        "equity": {
            "type": "number"
 
        },
        "company_handle": {
            "type": "string",
            "minLength": 1,
            "maxLength": 30
        }
    }
}